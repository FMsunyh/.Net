@{
  ViewBag.Title = "Create Menu";
}
<h2>Create Menu</h2>
<form action="/SubmitData/CreateMenu" method="post">
  <fieldset>
    <legend>Menu</legend>
    <div>Id:</div>
    <input name="id" />
